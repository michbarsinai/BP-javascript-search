<?xml version="1.0" encoding="UTF-8"?>
<!--
 Very simple, non-recursive chart.
-->
<lsc id="lsc101" name="Simple Message Exchange">
    <message from="a" to="b" content="ping!" temperature="hot" exec="execute" />
    <message from="b" to="a" content="pong!" temperature="hot" exec="execute" />
    <sync participants="a,b,c" />
    <message from="c" to="a" content="done" temperature="cold" exec="monitor" />
</lsc>
